{
  "$schema": "../gen/schemas/desktop-schema.json",
  "identifier": "default",
  "description": "Capability for the main window",
  "windows": [
    "main"
  ],
  "permissions": [
    "path:default",
    "path:allow-basename",
    "path:allow-dirname",
    "path:allow-extname",
    "path:allow-is-absolute",
    "path:allow-join",
    "path:allow-normalize",
    "path:allow-resolve",
    "path:allow-resolve-directory",
    "event:default",
    "window:default",
    "app:default",
    "resources:default",
    "menu:default",
    "tray:default",
    "shell:allow-open",
    "fs:allow-read-file",
    "fs:allow-write-file",
    "fs:allow-read-dir",
    "fs:allow-copy-file",
    "fs:allow-mkdir",
    "fs:allow-remove",
    "fs:allow-remove",
    "fs:allow-rename",
    "fs:allow-exists",
    "fs:read-all",
    "fs:write-all"
  ]
}